{
  "name": "@zulze12/eccrypto",
  "version": "1.1.2",
  "description": "JavaScript Elliptic curve cryptography library",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "ECCRYPTO_NO_FALLBACK=1 mocha && xvfb-run -a karma start && jshint .",
    "build": "node-pre-gyp build --target_arch=x64",
    "package-binary": "node-pre-gyp build package --target_arch=x64",
    "publish-binary": "node-pre-gyp publish --target_arch=x64",
    "m": "mocha",
    "k": "xvfb-run -a karma start",
    "kc": "xvfb-run -a karma start --browsers Chromium",
    "kf": "xvfb-run -a karma start --browsers Firefox",
    "j": "jshint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuptt/eccrypto.git"
  },
  "keywords": [
    "ecc",
    "ecdsa",
    "ecdh",
    "ecies",
    "crypto",
    "cryptography",
    "secp256k1",
    "K-256",
    "elliptic",
    "curve"
  ],
  "author": "Kagami Hiiragi",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/vuptt/eccrypto/issues"
  },
  "homepage": "https://github.com/vuptt/eccrypto",
  "binary": {
    "module_name": "ecdh",
    "module_path": "./lib/binding/bin/",
    "package_name": "{module_name}-{platform}-{arch}-{node_abi}.tar.gz",
    "host": "https://github.com/vuptt/eccrypto/releases/download/",
    "remote_path": "{version}"
  },
  "devDependencies": {
    "aws-sdk": "^2.556.0",
    "browserify": "16.5.0",
    "buffer-equal": "1.0.0",
    "chai": "*",
    "jshint": "*",
    "karma": "4.3.0",
    "karma-browserify": "6.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-firefox-launcher": "1.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "mocha": "*"
  },
  "dependencies": {
    "acorn": "7.1.0",
    "elliptic": "6.5.1",
    "es6-promise": "4.2.8",
    "nan": "2.14.0",
    "node-pre-gyp": "0.x",
    "secp256k1": "github:vuptt/secp256k1-node#node-pre-gyp"
  },
  "optionalDependencies": {
    "secp256k1": "github:vuptt/secp256k1-node#node-pre-gyp"
  },
  "directories": {
    "lib": "lib"
  }
}
